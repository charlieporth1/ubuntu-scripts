function lstunnel { 
    if [[ $1 == '-h' ]]; then
                echo 'Usage: lstunnel [REGEX]'
        else
                count=1
                pgrep -fl "ssh -fCNL $*" | cut -f2- -d ' ' | while read LINE; do
                    echo $count: $LINE;
                    count=`expr $count + 1`;
                done
        fi      
}

function rmtunnel {
    if [[ $* == '' ]] || [[ $1 == '-h' ]]; then
                echo 'Usage: rmtunnel N [REGEX]'
        else
                count=1
                pgrep -f "ssh -fCNL $2" | while read LINE; do
                    if [[ $1 == $count ]]; then
                                kill -9 $LINE;
                                break
                        fi
                        count=`expr $count + 1`;
                done
        fi
}

