    if [[ $* == '' ]] || [[ $1 == '-h' ]]; then
                echo 'Usage: rmtunnel N [REGEX]'
        else
                count=1
                pgrep -f "ssh -fCNL $2" | while read LINE; do
                    if [[ $1 == $count ]]; then
                                kill -9 $LINE;
                                break
                        fi
                        count=`expr $count + 1`;
